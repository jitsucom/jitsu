{
  "name": "jitsu-configurator-ui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@jitsu/catalog": "file:../catalog",
    "@jitsu/sdk-js": "^3.0.3",
    "@sentry/react": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "antd": "^4.13.1",
    "axios": "^0.21.4",
    "classnames": "^2.2.6",
    "firebase": "^9.4.1",
    "highlight.js": "^11.3.1",
    "hotkeys-js": "^3.8.7",
    "leader-line-new": "^1.1.9",
    "lodash": "^4.17.21",
    "logrocket": "^1.0.14",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "monaco-editor": "^0.28.1",
    "murmurhash": "^2.0.0",
    "posthog-js": "^1.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-monaco-editor": "^0.46.0",
    "react-reflex": "^4.0.3",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.4",
    "react-virtual": "^2.10.4",
    "recharts": "^2.0.8",
    "uuid": "^8.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-syntax-highlighter)",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "scripts": {
    "clean": "rm -rf ./node_modules/ build dist",
    "start": "yarn build:openapi && NODE_ENV=development craco start",
    "build": "yarn build:openapi && NODE_ENV=production craco build",
    "build:openapi": "openapi -i ../../../openapi/configurator.yaml -o src/generated/conf-openapi --exportCore false  --exportServices false  --exportModels true\n",
    "stats": "rm -rf build && yarn build -- --stats",
    "bundle": "npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "analyze": "source-map-explorer 'build/static/js/*.js' --no-border-checks",
    "test": "craco test --watchAll",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "immer": "^9.0.6",
    "path-parse": "^1.0.7",
    "ansi-regex": "^5.0.1",
    "tmpl": "^1.0.5",
    "set-value": "^4.1.0",
    "glob-parent": "^6.0.2",
    "postcss": "^7.0.39",
    "prismjs": "^1.25.0",
    "dns-packet": "^5.3.0",
    "ws": "^8.2.3",
    "tar": "^6.1.11",
    "nth-check": "^2.0.1",
    "browserslist": "^4.17.3"
  }
}
