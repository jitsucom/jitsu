    "dayjs": "^1.11.12",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "json5": "^2.2.3",
    "juava": "workspace:*",
    "kafkajs": "^2.2.4",
    "kafkajs-snappy": "^1.1.0",
    "node-cache": "^5.1.2",
    "node-fetch-commonjs": "^3.3.2",
    "object-hash": "^3.0.0",
    "p-queue": "^6.6.2",
    "pg": "^8.12.0",
    "pg-cursor": "^2.11.0",
    "prom-client": "^15.1.3",
    "tar": "^7.4.3",
    "tslib": "^2.6.3",
    "jsondiffpatch": "workspace:*",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
